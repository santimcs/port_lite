<% content_for :title, 'Dates' %>

<p id="notice"><%= notice %></p>

<h1>Latest Dates</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>

      <th>Edit</th>
    </tr>
  </thead>

  <tbody>
    <% @latest_dates.each do |latest_date| %>
      <tr>
        <td><%= latest_date.date %></td>
        <td><%= link_to_icon 'Edit', edit_latest_date_path(latest_date) %></td>         

      </tr>
    <% end %>
  </tbody>
</table>

